{
  "name": "@typescript-for-react-developers/workshop",
  "private": true,
  "prettier": {
    "printWidth": 80,
    "quoteProps": "consistent"
  },
  "dependencies": {
    "prettier": "^3.3.3"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md}\"",
    "lint": "pnpm run --parallel --filter \"*-valid\" lint",
    "prepared": "pnpm run --filter \"@workshop/template-valid\" dev",
    "typecheck": "pnpm run --parallel --filter \"*-valid\" typecheck"
  },
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c",
  "engines": {
    "node": ">=20.17.0"
  }
}
